{
  "name": "app-lang",
  "version": "2.7.3",
  "private": true,
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "sh ./tests/scripts/cypressRun.sh",
    "create:constant": "ts-node ./tests/scripts/createConstantCli.ts",
    "create:fixture": "ts-node ./tests/scripts/createFixtureCli.ts",
    "deleteactuals": "ts-node ./tests/scripts/deleteActuals.ts",
    "testWriteDiff": "ts-node ./tests/utils/testWriteDiff.ts",
    "testDeleteDiff": "ts-node ./tests/utils/testDeleteDiff.ts",
    "createRegressionDirs": "sh ./tests/scripts/createRegressionDirs.sh",
    "deleteBase": "ts-node ./tests/scripts/deleteBase.ts",
    "acceptChanges": "npm run deleteBase; sh ./tests/scripts/acceptChanges.sh",
    "showActuals": "ts-node ./tests/scripts/showActuals.ts",
    "showColors": "ts-node ./tests/scripts/showColors.ts",
    "start": "vue-cli-service serve",
    "start:test:e2e": "concurrently \"vue-cli-service serve\" \"npm run cy:run\"",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --verbose --watchAll --coverage --updateSnapshot --detectOpenHandles",
    "lint": "vue-cli-service lint"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/components/*.vue",
      "src/router/index.ts",
      "src/store/*.ts",
      "src/utils/*.ts",
      "src/views/*.vue",
      "src/App.vue",
      "src/main.ts"
    ]
  },
  "dependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@types/cypress": "^1.1.3",
    "@types/cypress-image-snapshot": "3.1.5",
    "@types/he": "^1.1.2",
    "@types/pixelmatch": "5.2.3",
    "@types/pngjs": "6.0.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-plugin-istanbul": "^6.1.1",
    "bulma": "^0.9.3",
    "concurrently": "^7.1.0",
    "core-js": "^3.8.3",
    "cypress": "^9.5.4",
    "cypress-file-upload": "5.0.8",
    "cypress-image-snapshot": "4.0.1",
    "he": "^1.2.0",
    "moment": "^2.29.2",
    "pngjs": "^6.0.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vue-router-mock": "^0.1.7",
    "vue-toastification": "^2.0.0-rc.5",
    "vuex": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "8.5.5",
    "@types/mocha": "^9.1.0",
    "@types/node": "15.12.4",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "istanbul-lib-coverage": "3.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "ts-node": "10.2.1",
    "typescript": "~4.5.5"
  },
  "devDependencies": {}
}