{
  "name": "server",
  "version": "1.4.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:config": "jest --showConfig",
    "test": "ENV_TXT=$(cat env.txt) jest --verbose --detectOpenHandles --watchAll",
    "test:coverage": "ENV_TXT=$(cat env.txt) jest --verbose --detectOpenHandles --watchAll --coverage",
    "server:ci": "jest --verbose --watchAll=false --detectOpenHandles --coverage",
    "tsw": "tsc -w",
    "tsc": "tsc",
    "watch": "cd dist; nodemon -L index.js",
    "start": "sh start.sh",
    "ts:run": "ts-node src/index.ts",
    "ts:watch": "nodemon src/index.ts",
    "dev": "sh dev.sh",
    "concurrently": "NODE_ENV=development ENV_TXT=$(cat env.txt) concurrently \"npm run tsw\" \"npm run watch\"",
    "gen": "graphql-codegen --config codegen.yml"
  },
